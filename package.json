{
  "name": "blogportfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "format": "prettier --write \"(pages|components|lib)/**/*.{ts,tsx}\"",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "lint-staged": {
    "*/*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.4.1",
    "@types/node": "^15.12.1",
    "@types/react": "^17.0.9",
    "dayjs": "^1.10.5",
    "globby": "^11.0.3",
    "gray-matter": "^4.0.3",
    "mdx-prism": "^0.3.3",
    "next": "10.2.3",
    "next-mdx-remote": "^3.0.2",
    "next-optimized-images": "^2.6.2",
    "next-themes": "^0.0.14",
    "preact": "^10.5.13",
    "prettier": "^2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "^1.3.0",
    "rehype": "^11.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-code-titles": "^0.1.2",
    "remark-slug": "^6.0.0",
    "rss": "^1.2.2",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.28.0",
    "eslint-config-next": "^10.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.0",
    "tailwindcss": "^2.1.4"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "parser": "typescript"
  }
}
